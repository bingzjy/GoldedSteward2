<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:background="@color/white"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <include layout="@layout/activity_browser_titlebar"/>
    <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                <TextView
                        android:id="@+id/tv_property_details_no"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingLeft="@dimen/dimen_14dp"
                        android:paddingTop="@dimen/dimen_5dp"
                        android:background="@color/white"
                        android:textColor="@color/tB2B5B7"
                        android:textSize="@dimen/font_size_14sp"/>
                <TextView
                        android:id="@+id/tv_property_details_status"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:lines="1"
                        android:padding="5dp"
                        android:layout_marginRight="@dimen/dimen_5dp"
                        android:text="SUBMIT"
                        android:textColor="@color/green"
                        android:textSize="@dimen/font_size_14sp"/>
            </LinearLayout>
            <TextView
                    android:id="@+id/tv_property_details_title"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/dimen_14dp"
                    android:paddingTop="@dimen/dimen_5dp"
                    android:background="@color/white"
                    android:text="没有找到与您相关的小区"
                    android:textColor="@color/black"
                    android:textSize="@dimen/font_size_16sp"/>
            <GridView
                    android:id="@+id/gv_list"
                    android:layout_width="wrap_content"
                    android:layout_height="64dp"
                    android:horizontalSpacing="3dp"
                    android:numColumns="5"
                    android:scrollbars="none"
                    android:verticalSpacing="5dp" >
            </GridView>
            <LinearLayout
                    android:layout_marginLeft="@dimen/dimen_14dp"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                <TextView
                        android:id="@+id/tv_property_details_type"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingTop="@dimen/dimen_5dp"
                        android:text="sdfasdf"
                        android:textColor="@color/tB2B5B7"
                        android:textSize="@dimen/font_size_14sp"/>
                <RatingBar
                        android:id="@+id/rb_score"
                        style="@style/rating_small_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:isIndicator="true"
                        android:layout_marginTop="5dp"
                        android:numStars="5"
                        android:rating="2"/>
                <TextView
                        android:id="@+id/tv_property_details_house"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingTop="@dimen/dimen_5dp"
                        android:text="sdfasdf"
                        android:textColor="@color/tB2B5B7"
                        android:textSize="@dimen/font_size_14sp"/>
                <TextView
                        android:id="@+id/tv_property_details_time"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingTop="@dimen/dimen_5dp"
                        android:gravity="right"
                        android:text="asfas"
                        android:textColor="@color/tB2B5B7"
                        android:textSize="@dimen/font_size_14sp"/>
            </LinearLayout>
            <LinearLayout
                    android:background="@color/white"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                <View
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="5dp"
                        android:background="@drawable/sharp_xuxian"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="详情"
                        android:textColor="@color/gray_deep_1"
                        android:textSize="@dimen/font_size_14sp"/>
                <View
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="5dp"
                        android:background="@drawable/sharp_xuxian"/>
            </LinearLayout>
            <com.ldnet.utility.MyListView
                    android:id="@+id/lv_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:cacheColorHint="@null"
                    android:background="@color/white"
                    android:layout_marginBottom="@dimen/dimen_10dp"
                    android:divider="@null">
            </com.ldnet.utility.MyListView>
        </LinearLayout>
    </ScrollView>
</LinearLayout>